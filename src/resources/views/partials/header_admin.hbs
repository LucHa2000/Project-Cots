<script
  src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"
></script>
<script>
  $(document).ready(function(){ var path = window.location.href $('.admin_nav li
  a').each(function(){ if (this.href === path) {
  $(this).parent('li').addClass('active') } else {
  $(this).parent('li').removeClass('active') } }) })
</script>